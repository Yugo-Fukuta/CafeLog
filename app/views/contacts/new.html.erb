<%= form_for @contact do |f| %>

<div class="form-group">
    <%= f.label :email, '返信先メールアドレス' %>
    <%= f.text_field :email, class: 'input-form' %>
</div>

<div class="form-group">
    <%= f.label :message, 'お問い合わせ内容' %>
    <%= f.text_area :message, class: 'input-area' %>
</div>

<%= f.submit '送信' %>

<% end %>